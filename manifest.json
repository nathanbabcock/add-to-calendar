{
  "manifest_version": 3,
  "name": "Add To Calendar",
  "version": "1.0",
  "background": {
    "service_worker": "src/service-worker.js"
  },
  "description": "Quickly and easily create Google calendar events by simply selecting the details and clicking on a button.",
  "permissions": ["storage", "tabs", "contextMenus"],
  "host_permissions": ["*://*/*"],
  "options_page": "src/options.html",
  "action": {
    "default_title": "Add To Calendar"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png"
  }
}
